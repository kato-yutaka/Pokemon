<p id="notice"><%= notice %></p>

<h1>技</h1>

<table>
  <thead>
    <tr>
      <th>パーティポケモン</th>
      <th>技名</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @buildskills.each do |buildskill| %>
      <tr>
        <td><%= buildskill.party_id %></td>
        <td><%= buildskill.skillname.name %></td>
        <td><%= link_to 'Show', buildskill %></td>
        <td><%= link_to 'Edit', edit_buildskill_path(buildskill) %></td>
        <td><%= link_to 'Destroy', buildskill, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

