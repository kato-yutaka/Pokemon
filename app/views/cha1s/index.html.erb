<p id="notice"><%= notice %></p>

<h1>Listing Cha1s</h1>

<table>
  <thead>
    <tr>
      <th>Cha</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cha1s.each do |cha1| %>
      <tr>
        <td><%= cha1.cha_id %></td>
        <td><%= link_to '詳細', cha1 %></td>
        <td><%= link_to '更新', edit_cha1_path(cha1) %></td>
        <td><%= link_to '削除', cha1, method: :delete, data: { confirm: '本当に削除しますか?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cha1', new_cha1_path %>
