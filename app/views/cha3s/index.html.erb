<p id="notice"><%= notice %></p>

<h1>Listing Cha3s</h1>

<table>
  <thead>
    <tr>
      <th>Cha</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cha3s.each do |cha3| %>
      <tr>
        <td><%= cha3.cha_id %></td>
        <td><%= link_to 'Show', cha3 %></td>
        <td><%= link_to 'Edit', edit_cha3_path(cha3) %></td>
        <td><%= link_to 'Destroy', cha3, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cha3', new_cha3_path %>
