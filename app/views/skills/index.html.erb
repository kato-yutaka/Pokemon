<p id="notice"><%= notice %></p>

<h1>技</h1>

<table>
  <thead>
    <tr>
      <th>育成論ID</th>
      <th>パーティポケモンID</th>
      <th>技名</th>
      <th>威力</th>
      <th>効果</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @skills.each do |skill| %>
      <tr>
        <td><%= skill.pokerais_id %></td>
        <td><%= skill.party_id %></td>
        <td><%= skill.name %></td>
        <td><%= skill.power %></td>
        <td><%= skill.eff %></td>
        <td><%= link_to '詳細', skill %></td>
        <td><%= link_to '編集', edit_skill_path(skill) %></td>
        <td><%= link_to '削除', skill, method: :delete, data: { confirm: '本当に削除しますか?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to '新規作成', new_skill_path %>
