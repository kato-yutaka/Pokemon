<p id="notice"><%= notice %></p>

<h1>技名</h1>

<table>
  <thead>
    <tr>
      <th>技名</th>
      <th>タイプ</th>
      <th>分類</th>
      <th>威力</th>
      <th>命中</th>
      <th>PP</th>
      <th>直接</th>
      <th>対象</th>
      <th>効果</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @skillnames.each do |skillname| %>
      <tr>
        <td><%= skillname.name %></td>
        <td><%= skillname.type %></td>
        <td><%= skillname.category %></td>
        <td><%= skillname.power %></td>
        <td><%= skillname.hit %></td>
        <td><%= skillname.pp %></td>
        <td><%= skillname.direct %></td>
        <td><%= skillname.target %></td>
        <td><%= skillname.eff %></td>
        <td><%= link_to '説明', skillname %></td>
        <td><%= link_to '編集', edit_skillname_path(skillname) %></td>
        <td><%= link_to '削除', skillname, method: :delete, data: { confirm: '本当に削除しますか？' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to '新規作成', new_skillname_path %>
